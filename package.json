{
  "name": "react-save-refs",
  "version": "1.1.0",
  "description": "Utility for keeping references to multiple React elements.",
  "main": "js/index.js",
  "types": "index.d.ts",
  "sideEffects": false,
  "scripts": {
    "prepare": "rimraf js/ && babel -s inline -d js/ src/ && flow-copy-source -v src js",
    "test": "yarn run lint && yarn run flow_check && mocha && tsc",
    "flow_check": "flow check",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Macil/react-save-refs.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Chris Cowan <agentme49@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Macil/react-save-refs/issues"
  },
  "homepage": "https://github.com/Macil/react-save-refs#readme",
  "dependencies": {
    "lodash": "^4.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.0",
    "babel-plugin-add-module-exports": "^1.0.0",
    "eslint": "^5.5.0",
    "flow-bin": "^0.80.0",
    "flow-copy-source": "^2.0.0",
    "mocha": "^6.0.0",
    "rimraf": "^2.6.2",
    "typescript": "^3.0.3"
  },
  "greenkeeper": {
    "ignore": [
      "flow-bin"
    ]
  }
}
